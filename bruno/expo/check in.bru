meta {
  name: check in
  type: http
  seq: 1
}

put {
  url: {{url}}/api/expo/check-in/S-000003
  body: none
  auth: inherit
}

script:pre-request {
  const cookie = bru.getVar("cookie");
  
  if(cookie) {
    req.setHeader("Cookie", cookie)
  }
}

script:post-response {
  const cookies = res.getHeader('set-cookie');
  
  if(cookies) {
    bru.setVar("cookie", cookies.join('; '));
  }
}
